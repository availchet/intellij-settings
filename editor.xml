<application>
  <component name="DaemonCodeAnalyzerSettings">
    <option name="SHOW_ADD_IMPORT_HINTS" value="false" />
  </component>
  <component name="EditorSettings">
    <option name="STRIP_TRAILING_SPACES" value="Whole" />
  </component>
  <component name="InlayHintsSettings">
    <option name="lastViewedProviderKeyId" value="annotation.hints" />
    <settingsMapElement>
      <JAVA.annotation.hints>
        <Settings>
          <option name="showInferred" value="true" />
